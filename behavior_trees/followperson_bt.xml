<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <ReactiveSequence>
            <ReactiveFallback>
                <Action ID="ifperson" name="IfPerson" speed="speed"/>
                <Action ID="Move" speed="speed"/>
            </ReactiveFallback>
            <Action ID="Move" speed="speed"/>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Move">
            <input_port name="speed"/>
        </Action>
        <Action ID="ifperson">
            <output_port name="speed"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

